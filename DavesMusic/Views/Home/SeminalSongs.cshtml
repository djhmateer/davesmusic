@model List<DavesMusic.Controllers.SongsVM>

<script type="text/javascript" src="../../Scripts/jquery-1.10.2.min.js"></script>
<script type="text/javascript">
    function go() {
        $.get("@Url.Action("GetGreeting")" ,
            {
                name: "Joe"
            }, function (data){
                alert(data);
            }
        );
    }

    go();
</script>

<h2>Seminal Songs</h2>

<!-- make it so that this passes a null? so can get current users id in the controller-->
<a href="/Playlists/Create/davemateer">Make new playlist in my Spotify</a>
<p>Great songs of all time!  If the entire album is worth it, put it on that playlist too.</p>
<table>
    @foreach (var item in Model) {
        <tr>
            <td>@item.TrackName</td>
            <td>@item.ArtistName</td>
            <td>


            </td>
        </tr>
    }
</table>

