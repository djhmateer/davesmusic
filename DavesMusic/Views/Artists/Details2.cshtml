@model DavesMusic.Controllers.ArtistAlbums

<h3>Studio Albums (@Model.items.Count)</h3>
<table class="table">
    @if (Model != null) {
        for (int i = 0; i < Model.items.Count; i++) {
            @*@Html.HiddenFor(model => Model.ArtistAlbums[i].id)
            @Html.HiddenFor(model => Model.ArtistAlbums[i].name)*@

            <tr>
                <td>@Model.items[i].name</td>
                <td>@Html.EditorFor(model => Model.items[i].Checked)</td>
                <td><a href="@Model.items[i].uri">Direct</a></td>
                <td>@*@Model.items[i].release_date*@</td>
                <td>
                    <a href="/Albums/Details/@Model.items[i].id">
                        @if (Model.items[i].images.Count > 2) {
                            <img src="@Model.items[i].images[2].url" />
                        }

                    </a>
                </td>
            </tr>
        }
    }
</table>





